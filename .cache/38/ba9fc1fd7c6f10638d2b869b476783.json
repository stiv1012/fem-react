ame":"Component","generated":{"line":125,"column":17},"source":"Details.js","original":{"line":11,"column":28}},{"name":"Details","generated":{"line":127,"column":15},"source":"Details.js","original":{"line":72,"column":15}}],"sources":{"Details.js":"import React from \"react\";\r\nimport pf from \"petfinder-client\";\r\nimport { navigate } from \"@reach/router\";\r\nimport Carousel from \"./Caroussel\";\r\n\r\nconst petfinder = pf({\r\n  key: process.env.API_KEY,\r\n  secret: process.env.API_SECRET\r\n});\r\n\r\nclass Details extends React.Component {\r\n  state = { loading: true, showModal: false };\r\n  componentDidMount() {\r\n    petfinder.pet\r\n      .get({\r\n        output: \"full\",\r\n        id: this.props.id\r\n      })\r\n      .then(data => {\r\n        let breed;\r\n        if (Array.isArray(data.petfinder.pet.breeds.breed)) {\r\n          breed = data.petfinder.pet.breeds.breed.join(\", \");\r\n        } else {\r\n          breed = data.petfinder.pet.breeds.breed;\r\n        }\r\n        this.setState({\r\n          name: data.petfinder.pet.name,\r\n          animal: data.petfinder.pet.animal,\r\n          location: `${data.petfinder.pet.contact.city}, ${\r\n            data.petfinder.pet.contact.state\r\n            }`,\r\n          description: data.petfinder.pet.description,\r\n          media: data.petfinder.pet.media,\r\n          breed,\r\n          loading: false\r\n        });\r\n      })\r\n      .catch(() => {\r\n        navigate(\"/\");\r\n      });\r\n  }\r\n  toggleModal = () => this.setState({ showModal: !this.state.showModal });\r\n  render() {\r\n    if (this.state.loading) {\r\n      return <h1>loading … </h1>;\r\n    }\r\n\r\n    const {\r\n      media,\r\n      animal,\r\n      breed,\r\n      location,\r\n      description,\r\n      name,\r\n      showModal\r\n    } = this.state;\r\n\r\n    return (\r\n      <div className=\"details\">\r\n        <Carousel media={media} />\r\n        <div>\r\n          <h1>{name}</h1>\r\n          <h2>{`${animal} — ${breed} — ${location}`}</h2>\r\n          <button onClick={this.toggleModal}>Adopt {name}</button>\r\n          <p>{description}</p>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Details;"},"lineCount":null}},"hash":"89ba2755e2348cd31d4d888ed0a26629","cacheData":{"env":{"API_KEY":"384b0d26b379a5b99893c6f4cda10b91","API_SECRET":"fb39371f063a8859751db7f7c758bb87"}}}